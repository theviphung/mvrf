<!-- Title -->
<h1>Sign Up for mvrf!</h1>

<!-- Blurb -->
<p>Let's get started! Input your info below.</p>

<div class="form-container">
    <h2>Sign Up</h2>
    <%= form_for @user do |f| %>
        <%= f.label :username %>
        <%= f.text_field :username %><br>
        <%= f.label :password %>
        <%= f.password_field :password %><br>
        <%= f.submit "Create Account" %>
    <% end %>
</div>



</body>
</html>

<h1>Contact Us</h1>

<%= form_for :contact, url: contact_path do |f| %>
    <p>
        <%= f.label :name%> <br>
        <%= f.text_field :name%>
    </p>

    <p>
        <%= f.label :email%> <br>
        <%= f.email_field :email%>
    </p>

    <p>
        <%= f.label :content%> <br>
        <%= f.text_area :content%>
    </p>

    <p>
        <%= f.submit "Submit" %>
    </p>
<%end%>